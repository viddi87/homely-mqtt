{
  "plugins": ["prettier", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
  "root": true,
  "rules": {
    "prettier/prettier": "warn",
    "@typescript-eslint/no-unused-vars": ["error", { "ignoreRestSiblings": true }]
  }
}